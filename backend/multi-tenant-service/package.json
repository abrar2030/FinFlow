    "jsonwebtoken": "^9.0.1",
    "bcryptjs": "^2.4.3",
    "uuid": "^9.0.0",
    "lodash": "^4.17.21",
    "moment": "^2.29.4",
    "dotenv": "^16.3.1",
    "winston": "^3.10.0",
    "joi": "^17.9.2",
    "knex": "^2.5.1",
    "pg": "^8.11.1",
