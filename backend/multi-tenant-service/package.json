{
  "name": "multi-tenant-service",
  "version": "1.0.0",
  "description": "Multi-tenant architecture service for FinFlow enterprise clients",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "start:dev": "ts-node-dev --respawn --transpile-only src/index.ts",
